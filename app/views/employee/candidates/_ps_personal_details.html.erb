<div class='collapse-group'>
  <div class='collapse-heading'>
    <%= link_to 'Personal Details', '#', class: 'collapse-toggle', data: { toggle: 'collapse', target: ".collapse-personal-details" } %>
  </div>
  <div class='collapse collapse-personal-details'>
    <div class='collapse-content'>
      <table class='table table-bordered table-striped'>
        <tbody>
          <tr>
            <td class='attr'>Name</td>
            <td><%= @peoplesoft_candidate.full_name %></td>
          </tr>
          <tr>
            <td class='attr'>HCC Student ID</td>
            <td><%= @peoplesoft_candidate.emplid %></td>
          </tr>
          <tr>
            <td class='attr'>Gender</td>
            <td><%= @peoplesoft_candidate.sex %></td>
          </tr>
          <tr>
            <td class='attr'>Ethnicity</td>
            <td><%= @peoplesoft_candidate.details[:h_ethnic_grp_list] %></td>
          </tr>
          <tr>
            <td class='attr'>Citizenship</td>
            <td><%= @peoplesoft_candidate.details[:citizenship_status] %> - <%= @peoplesoft_candidate.citizenship %></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>