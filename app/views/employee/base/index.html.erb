<div class="row dashboard">
  <%= render 'employees' if current_user.is_able_to :view, Employee.new %>
  <%= render 'candidates' if current_user.is_able_to :view, Candidate.new %>
  <%= render 'program_sessions' if current_user.is_able_to :view, ProgramDate.new %>
  <%= render 'reports' if current_user.can_view_reports? %>
  <%= render 'system_settings' if
					current_user.is_able_to(:edit, DisciplineTeam) or
					current_user.is_able_to(:edit, Program) or
					current_user.is_able_to(:edit, PathwaySetting) %>
</div>
